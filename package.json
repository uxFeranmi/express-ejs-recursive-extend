{
  "name": "ejs-with-layouts",
  "version": "0.0.2",
  "description": "Adds support for Nested Layouts to EJS templates. Compatible with Express.js.",
  "keywords": [
    "express",
    "ejs",
    "template",
    "layout"
  ],
  "scripts": {
    "test-server": "nodemon test/server.js"
  },
  "nodemonConfig": {
    "watch": [
      "**/*.js",
      "**/*.ejs"
    ],
    "ignore": ["**/node_modules/**"],
    "ext": "js,json,ejs"
  },
  "homepage": "https://github.com/uxFeranmi/express-ejs-recursive-extend",
  "bugs": {
    "url": "https://github.com/uxFeranmi/express-ejs-recursive-extend/issues"
  },
  "license": "MIT",
  "author": "Feranmi Akinlade <uxFeranmi@gmail.com> (https://feranmi.dev)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uxFeranmi/express-ejs-recursive-extend.git"
  },
  "dependencies": {
    "ejs": "^3.1.3"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.22"
  }
}
